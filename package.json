{
    "name": "piconvert",
    "version": "0.4.2",
    "description": "Convert pictures to other formats. Supports ai, cdr, vsd, pdf, and more formats. GitHub Actions & CLI Tool.",
    "main": "dist/index.js",
    "types": "dist/index.d.ts",
    "bin": {
        "piconvert": "dist/piconvert.js"
    },
    "scripts": {
        "test": "jest --coverage",
        "dev": "tsup --watch",
        "prepare": "npm run build",
        "build": "tsup",
        "docs": "typedoc ./src/",
        "format": "prettier --write '**/*.{js,ts,jsx,tsx,json,yml,yaml,md,html}' '!dist' '!coverage' '!docs'",
        "lint": "eslint ."
    },
    "keywords": [
        "AI",
        "Adobe Illustrator",
        "Convert",
        "SVG",
        "Inkscape",
        "GitHub Actions"
    ],
    "author": "JacobLinCool <jacoblincool@gmail.com> (https://github.com/JacobLinCool)",
    "license": "MIT",
    "files": [
        "dist"
    ],
    "devDependencies": {
        "@types/jest": "^27.4.0",
        "@types/js-yaml": "4.0.5",
        "@types/node": "17.0.14",
        "@typescript-eslint/eslint-plugin": "5.10.2",
        "@typescript-eslint/parser": "5.10.2",
        "eslint": "8.8.0",
        "eslint-config-prettier": "8.3.0",
        "jest": "27.4.7",
        "prettier": "2.5.1",
        "ts-jest": "27.1.3",
        "tsup": "5.11.13",
        "typedoc": "0.22.11",
        "typescript": "4.5.5"
    },
    "dependencies": {
        "chalk": "4.1.2",
        "commander": "^9.0.0",
        "js-yaml": "^4.1.0"
    },
    "homepage": "https://github.com/JacobLinCool/piconvert",
    "bugs": {
        "url": "https://github.com/JacobLinCool/piconvert/issues"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/JacobLinCool/piconvert.git"
    }
}
