{
    "name": "piconvert",
    "version": "0.5.2",
    "description": "Convert pictures to other formats. Supports ai, cdr, vsd, pdf, and more formats. GitHub Actions & CLI Tool.",
    "main": "dist/index.js",
    "types": "dist/index.d.ts",
    "bin": {
        "piconvert": "dist/piconvert.js"
    },
    "scripts": {
        "test": "jest --coverage",
        "dev": "tsup --watch",
        "prepare": "npm run build",
        "build": "tsup",
        "docs": "typedoc ./src/",
        "format": "prettier --write . --ignore-path .gitignore",
        "lint": "eslint ."
    },
    "keywords": [
        "AI",
        "Adobe Illustrator",
        "Convert",
        "SVG",
        "Inkscape",
        "GitHub Actions"
    ],
    "author": "JacobLinCool <jacoblincool@gmail.com> (https://github.com/JacobLinCool)",
    "license": "MIT",
    "files": [
        "dist"
    ],
    "devDependencies": {
        "@trivago/prettier-plugin-sort-imports": "3.4.0",
        "@types/jest": "29.2.0",
        "@types/js-yaml": "4.0.5",
        "@types/node": "18.11.5",
        "@types/sharp": "0.31.0",
        "@types/svgo": "2.6.4",
        "@typescript-eslint/eslint-plugin": "5.41.0",
        "@typescript-eslint/parser": "5.41.0",
        "eslint": "8.26.0",
        "eslint-config-prettier": "8.5.0",
        "jest": "29.2.2",
        "prettier": "2.7.1",
        "ts-jest": "29.0.3",
        "tsup": "6.3.0",
        "typedoc": "0.23.18",
        "typescript": "4.8.4"
    },
    "dependencies": {
        "chalk": "4.1.2",
        "commander": "^9.4.1",
        "js-yaml": "^4.1.0",
        "sharp": "^0.31.1",
        "svgo": "^2.8.0"
    },
    "homepage": "https://github.com/JacobLinCool/piconvert",
    "bugs": {
        "url": "https://github.com/JacobLinCool/piconvert/issues"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/JacobLinCool/piconvert.git"
    }
}
