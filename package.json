{
    "name": "piconvert",
    "version": "0.1.1",
    "description": "Convert AI (Adobe Illustrator) pictures to other formats.",
    "main": "dist/index.js",
    "bin": {
        "piconvert": "dist/index.js"
    },
    "scripts": {
        "test": "jest --coverage",
        "dev": "tsup --watch",
        "build": "tsup",
        "format": "prettier --write '**/*.{js,ts,jsx,tsx,json,yml,yaml,md,html}' '!dist' '!coverage'",
        "lint": "eslint ."
    },
    "keywords": [
        "AI",
        "Adobe Illustrator",
        "Convert",
        "SVG",
        "PNG"
    ],
    "author": "JacobLinCool <jacoblincool@gmail.com> (https://github.com/JacobLinCool)",
    "license": "MIT",
    "files": [
        "dist"
    ],
    "devDependencies": {
        "@types/jest": "^27.4.0",
        "@types/node": "^17.0.10",
        "@typescript-eslint/eslint-plugin": "^5.10.0",
        "@typescript-eslint/parser": "^5.10.0",
        "eslint": "^8.7.0",
        "eslint-config-prettier": "^8.3.0",
        "jest": "^27.4.7",
        "prettier": "^2.5.1",
        "ts-jest": "^27.1.3",
        "tsup": "^5.11.11",
        "typescript": "^4.5.5"
    },
    "dependencies": {
        "chalk": "4.1.2",
        "commander": "^9.0.0"
    }
}
